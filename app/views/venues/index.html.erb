<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>State</th>
    <th>Family Friendly?</th>
  </tr>
 
  <% @venue.each do |t| %>
    <tr>
      <td><%= t.name %></td>
      <td><%= t.city %></td>
      <td><%= t.state %></td>
      <td><%= t.familyfriendly %></td>
      <td><%= link_to 'Show', venue_path(t) %></td>
    </tr>
  <% end %>
</table>